
/* $Id$ */

digraph slavescan {
    size="7,9"
    center=1
	ratio=fill

    start -> address [weight=10]

    address -> error
    address -> state [weight=10]

    state -> error
    state -> base [weight=10]

    base -> error
    base -> datalink [weight=10]

    datalink -> error
    datalink -> sii_size [weight=10]

    sii_size -> error
    sii_size -> sii_data [weight=10]

    sii_data -> error
    sii_data -> end [weight=10]
}
